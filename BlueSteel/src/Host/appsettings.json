{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },

  "Actuators": {
    "Enable": true,

    "Routes": [
      {
        "Type": "HealthActuator",
        "Path": "/health",
        "Settings": {
          "IncludeVersion": true,
          "IncludeUpTime": true,
          "IncludeAccesses": true
        }
      }
    ]
  }
}
